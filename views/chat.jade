extends layout

block head
    link(rel='stylesheet', href='/stylesheets/chat.css')
block content
    h1= title
    p Welcome to #{title}
    ul(id='messages')
    form(action='#{script}')
        input(id='m', autocomplete=off)
        button Send
    script(src="https://cdn.socket.io/socket.io-1.4.5.js")
    script(src="http://code.jquery.com/jquery-1.11.1.js")
    script(src="/javascripts/chat.js")
