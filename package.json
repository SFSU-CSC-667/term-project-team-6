    "pug": "^2.0.0-beta6",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.0",
    "socket.io": "latest",
    "pg-promise": "latest",
    "bluebird": "latest"
  },
  "devDependencies": {
    "babel-preset-latest": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "nodemon": "^1.11.0",
    "watchify": "^3.7.0"
